<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BaseBrush" Color="#FAF3F3" />
    <Color x:Key="BaseColor">#FAF3F3</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#E1E5EA" />
    <Color x:Key="MediumBaseColor">#E1E5EA</Color>

    <SolidColorBrush x:Key="BorderColorBrush" Color="#A7BBC7" />
    <Color x:Key="BorderColor">#A7BBC7</Color>

    <SolidColorBrush x:Key="ArithmeticBottonBrush" Color="#FF134496" />
    <Color x:Key="ArithmeticBottonColor">#FF134496</Color>

    <SolidColorBrush x:Key="DeleteBottonBrush" Color="Red" />
    <Color x:Key="DeleteBottonColor">Red</Color>
</ResourceDictionary>